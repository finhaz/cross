<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="cross.EthernetDataView">
      <Border Background="#F3F4F6" Padding="20">
        <Grid>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="20">
                <TextBlock Text="以太网数据管理页面" FontSize="28" Foreground="#1F2937"/>
                <TextBlock Text="• IP地址：192.168.1.100" FontSize="18"/>
                <TextBlock Text="• 端口号：8080" FontSize="18"/>
                <TextBlock Text="• 协议：TCP" FontSize="18"/>
                <Button Content="连接服务器" Width="120" Margin="0 20 0 0"/>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
